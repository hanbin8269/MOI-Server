generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model sessions {
  session_id String  @id
  expires    Int
  data       String?
}

model User {
  email    String  @id
  password String?
}

model Project {
  project_id Int @id
  title String
  content String
  github_link String
  users User[]
  techs Tech[]
}

model Tech {
  tech_id Int @id
  name String
  count Int
}