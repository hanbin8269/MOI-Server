generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model sessions {
  session_id String  @id
  expires    Int
  data       String?
}

model user {
  email    String  @id
  password String?
}

model project {
  project_id Int @id
  title String
  content String
  github_link String
  users user[]
  techs tech[]
}

model tech {
  tech_id Int @id
  name String
  count Int
}